{
  "name": "NextLens.io",
  "homepage": "https://www.nextlens.io/",
  "version": "1.0.0",
  "description": "Lens Recommendations Web App",
  "author": {
    "name": "Devon Yu",
    "email": "devonyu415@gmail.com",
    "url": "https://devonyu.com"
  },
  "engines": {
    "node": "12.14.1"
  },
  "scripts": {
    "start": "node server/server.js",
    "test": "node_modules/.bin/jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "testing": "jest --watch",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.11.0",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "express": "^4.17.3",
    "express-session": "^1.15.6",
    "flickrapi": "^0.6.0",
    "lodash": "^4.17.10",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "redis": "^2.8.0",
    "rimraf": "^2.6.2",
    "semantic-ui-react": "^0.82.1",
    "uuid": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devonyu/nextLens-io"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react",
    "photography",
    "camera",
    "lens",
    "recommendation",
    "next",
    "nextLens",
    "dslr"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "jest-enzyme": "^6.0.2",
    "jshint": "^2.6.0",
    "nodemon": "^1.18.3",
    "react-test-renderer": "^16.4.1",
    "regenerator-runtime": "^0.12.0"
  }
}